    "react-hot-loader": "3.1.1",
    "start-server-webpack-plugin": "2.2.0",
    "webpack": "3.8.1",
    "webpack-dashboard": "1.0.0",
    "webpack-dev-server": "2.9.3",
    "webpack-merge": "4.1.1",
    "webpack-node-externals": "1.6.0"
  }
