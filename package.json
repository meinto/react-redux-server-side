    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-icons": "^2.2.5",
    "react-jss": "7.2.0",
    "react-markdown": "2.5.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-scripts": "1.0.16",
    "redux": "3.7.2",
    "redux-observable": "0.17.0",
