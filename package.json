  "scripts": {
    "start": "node ./dist/server.js",
    "forever:stop": "forever stop ./dist/server.js",
    "forever:start": "cross-env NODE_ENV=production forever start ./dist/server.js",
    "forever:restart": "forever restart ./dist/server.js",
    "dev": "webpack-dashboard -- webpack-dev-server --config webpack.config.dev.js --watch --inline --colors  --port 8080 --hot --history-api-fallback",
    "server:dev:mon": "nodemon --exec cross-env NODE_ENV=development babel-node ./src/server",
    "server:dev": "rm -rf ./dist && npm run build:dist && cross-env NODE_ENV=development npm start",
    "build:dist": "npm-run-all --parallel build:client build:server",
    "build:client": "webpack --config  webpack.config.client.js",
    "build:server": "webpack --config  webpack.config.server.js",
    "generate:sitemap": "babel-node ./src/server/sitemap-generator.js",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "6.24.1",
